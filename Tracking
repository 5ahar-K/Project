      "cell_type": "code",
      "source": [
        "# cubesat_apd_model.py\n",
        "from __future__ import annotations\n",
        "import numpy as np\n",
        "from scipy.special import erfc\n",
        "\n",
        "# ---- Q-function (same meaning as in Mansour) ----\n",
        "def qfunc(x):\n",
        "    return 0.5 * erfc(x / np.sqrt(2.0))\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# Step 2: APD-array spatial channel h2_ij (CubeSat paper eq. 17)\n",
        "# =============================================================================\n",
        "def h2_ij_array(\n",
        "    Na: int,\n",
        "    wa: float,\n",
        "    wf: float,\n",
        "    fc: float,\n",
        "    sigma_I: float,\n",
        "    theta_x: float,\n",
        "    theta_y: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    Implements h2,ij in eq. (17): product of Q-function differences. :contentReference[oaicite:4]{index=4}\n",
        "\n",
        "    Units:\n",
        "      wa, wf, fc, sigma_I in meters\n",
        "      theta_x, theta_y in radians\n",
        "\n",
        "    Returns:\n",
        "      h2: (Na, Na) nonnegative fractions (not forced to sum to 1)\n",
        "    \"\"\"\n",
        "    w_ap = wa + wf  # w'_a\n",
        "    h2 = np.zeros((Na, Na), dtype=float)\n",
        "\n",
        "    for i in range(1, Na + 1):\n",
        "        x1 = (i - 1 - Na / 2.0) * w_ap + wf / 2.0\n",
        "        x2 = (i - Na / 2.0) * w_ap - wf / 2.0\n",
        "        ax = qfunc((x1 - fc * theta_x) / sigma_I) - qfunc((x2 - fc * theta_x) / sigma_I)\n",
        "\n",
        "        for j in range(1, Na + 1):\n",
        "            y1 = (j - 1 - Na / 2.0) * w_ap + wf / 2.0\n",
        "            y2 = (j - Na / 2.0) * w_ap - wf / 2.0\n",
        "            ay = qfunc((y1 - fc * theta_y) / sigma_I) - qfunc((y2 - fc * theta_y) / sigma_I)\n",
        "\n",
        "            h2[i - 1, j - 1] = ax * ay\n",
        "\n",
        "    # numeric safety\n",
        "    h2[h2 < 0] = 0.0\n",
        "    return h2\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# Step 3: Full APD channel matrix hij = h1 * h2_ij  (eq. 16) :contentReference[oaicite:5]{index=5}\n",
        "# =============================================================================\n",
        "def channel_hij(Na, wa, wf, fc, sigma_I, theta_x, theta_y, h1):\n",
        "    h2 = h2_ij_array(Na, wa, wf, fc, sigma_I, theta_x, theta_y)\n",
        "    return h1 * h2\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# Step 4: Received signal simulation (eq. 18-19) :contentReference[oaicite:6]{index=6}\n",
        "# =============================================================================\n",
        "def simulate_window(\n",
        "    L: int,\n",
        "    hij: np.ndarray,          # (Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,          # coefficient (NOT including Pt)\n",
        "    sigma0_2: float,\n",
        "    rng: np.random.Generator,\n",
        "    Pt_W: float,              # <-- NEW: optical power for bit-1 (watts)\n",
        "    bits: np.ndarray | None = None,\n",
        ") -> tuple[np.ndarray, np.ndarray]:\n",
        "    \"\"\"\n",
        "    CubeSat paper model:\n",
        "      r_ij[k] = mu*hij*s[k] + n_ij[k]\n",
        "      Var = sigma_s2*hij*s[k] + sigma0_2\n",
        "    where s[k] is OOK symbol WITH optical power Pt (i.e., 0 or Pt). :contentReference[oaicite:2]{index=2}\n",
        "    \"\"\"\n",
        "    Na = hij.shape[0]\n",
        "    if bits is None:\n",
        "        b = rng.integers(0, 2, size=L, dtype=np.int8)   # bits\n",
        "    else:\n",
        "        b = bits.astype(np.int8)\n",
        "\n",
        "    # OOK optical power sequence: s[k] ∈ {0, Pt_W}\n",
        "    s = b.astype(float) * Pt_W\n",
        "\n",
        "    r = np.empty((L, Na, Na), dtype=float)\n",
        "    for k in range(L):\n",
        "        mean = mu * hij * s[k]\n",
        "        var  = sigma_s2 * hij * s[k] + sigma0_2\n",
        "        noise = rng.normal(0.0, np.sqrt(var), size=(Na, Na))\n",
        "        r[k] = mean + noise\n",
        "\n",
        "    return b, r\n"
      ],
      "metadata": {
        "id": "aEusPnUWJ4AU"
      },
      "execution_count": 33,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# =============================================================================\n",
        "# Step 5A: Ideal detector (perfect hij), symbol-by-symbol ML using eq. (18-19)\n",
        "# =============================================================================\n",
        "def detect_ideal_symbolwise(\n",
        "    r: np.ndarray,            # (L,Na,Na)\n",
        "    hij: np.ndarray,          # (Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    Ideal Rx: hij known. Uses per-bit log-likelihood comparing s=0 vs s=1\n",
        "    under eq. (18)-(19). :contentReference[oaicite:8]{index=8}\n",
        "    \"\"\"\n",
        "    L = r.shape[0]\n",
        "    s_hat = np.zeros(L, dtype=np.int8)\n",
        "\n",
        "    # s=0 model: mean=0, var=sigma0_2\n",
        "    const0 = np.log(2.0 * np.pi * sigma0_2)\n",
        "\n",
        "    for k in range(L):\n",
        "        rk = r[k]\n",
        "\n",
        "        # log p(r|s=0)\n",
        "        ll0 = -0.5 * np.sum(const0 + (rk ** 2) / sigma0_2)\n",
        "\n",
        "        # log p(r|s=1) (mean mu*h, var sigma_s2*h + sigma0_2)\n",
        "        v1 = sigma_s2 * hij + sigma0_2\n",
        "        ll1 = -0.5 * np.sum(np.log(2.0 * np.pi * v1) + ((rk - mu * hij) ** 2) / v1)\n",
        "\n",
        "        s_hat[k] = 1 if ll1 > ll0 else 0\n",
        "\n",
        "    return s_hat\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# Step 5B: EGC detector (eq. 36-40) :contentReference[oaicite:9]{index=9}\n",
        "# =============================================================================\n",
        "def egc_threshold(ht: float, Na: int, mu: float, sigma_s2: float, sigma0_2: float) -> float:\n",
        "    \"\"\"\n",
        "    Implements eq. (40) threshold h_th,EGC. :contentReference[oaicite:10]{index=10}\n",
        "    \"\"\"\n",
        "    # Protect against divide-by-zero\n",
        "    if ht <= 0:\n",
        "        return np.inf\n",
        "\n",
        "    A = (Na**2) * sigma0_2\n",
        "    B = sigma_s2 * ht\n",
        "\n",
        "    term1 = -mu * ht * A / B\n",
        "    term2 = np.sqrt((A + B) * A / B) * np.sqrt(mu**2 * ht**2 + 2.0 * B * np.log((A + B) / A))\n",
        "    return term1 + term2\n",
        "\n",
        "\n",
        "def detect_egc_symbolwise(\n",
        "    r: np.ndarray,            # (L,Na,Na)\n",
        "    hij: np.ndarray,          # (Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    EGC: r_sum[k] = sum_ij r_ij[k], ht = sum_ij h_ij, then threshold. :contentReference[oaicite:11]{index=11}\n",
        "    \"\"\"\n",
        "    L, Na, _ = r.shape\n",
        "    ht = float(np.sum(hij))\n",
        "    thr = egc_threshold(ht, Na, mu, sigma_s2, sigma0_2)\n",
        "\n",
        "    r_sum = np.sum(r, axis=(1, 2))\n",
        "    return (r_sum > thr).astype(np.int8)\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# Step 5C: Proposed GLRT detector\n",
        "#   Uses eq. (41)-(44) for channel estimation given a candidate s\n",
        "#   Uses reduced search among L candidates (top-m samples assumed '1') :contentReference[oaicite:13]{index=13}\n",
        "# =============================================================================\n",
        "def _hopt_from_Rij(Rij: float, mu: float, sigma_s2: float, sigma0_2: float) -> float:\n",
        "    \"\"\"\n",
        "    Implements h_opt from eq. (44). :contentReference[oaicite:14]{index=14}\n",
        "    \"\"\"\n",
        "    # eq. (44):\n",
        "    # h_opt = -sigma_s2/(2 mu^2) - sigma0_2/sigma_s2 + sqrt( sigma_s2^2/(4 mu^4) + sigma0_2^2/sigma_s2^2 + Rij )\n",
        "    inside = (sigma_s2**2) / (4.0 * mu**4) + (sigma0_2**2) / (sigma_s2**2) + Rij\n",
        "    inside = max(inside, 0.0)\n",
        "    return -sigma_s2 / (2.0 * mu**2) - (sigma0_2 / sigma_s2) + np.sqrt(inside)\n",
        "\n",
        "\n",
        "def _glrt_metric_for_candidate_s(\n",
        "    r: np.ndarray,            # (L,Na,Na)\n",
        "    s: np.ndarray,            # (L,) 0/1\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> float:\n",
        "    \"\"\"\n",
        "    Computes the GLRT negative log-likelihood metric:\n",
        "      sum_{i,j} [A1(i,j) + A2(i,j; h_opt)]\n",
        "    where A1, A2 are from eq. (42)-(43) and h_opt from eq. (44).\n",
        "\n",
        "    NOTE: This avoids re-typing eq. (45) (formatting in PDFs can be tricky) and\n",
        "    stays faithful to the derivation.\n",
        "    \"\"\"\n",
        "    s = s.astype(np.int8)\n",
        "    L, Na, _ = r.shape\n",
        "    m = int(np.sum(s))\n",
        "    if m == 0:\n",
        "        return np.inf  # paper notes all-zero window is problematic :contentReference[oaicite:16]{index=16}\n",
        "\n",
        "    K1 = (s == 1)\n",
        "    K0 = ~K1\n",
        "\n",
        "    # A1: for s=0 samples (independent of hij): ln(sigma0^2) + |r|^2/sigma0^2\n",
        "    # This is repeated per APD and per k in K0 (eq. 42). :contentReference[oaicite:17]{index=17}\n",
        "    metric = 0.0\n",
        "    metric += (L - m) * (Na**2) * np.log(sigma0_2)\n",
        "    metric += np.sum((r[K0] ** 2) / sigma0_2)\n",
        "\n",
        "    # For each APD, compute h_opt using eq. (44) with Rij from eq. (44) definition :contentReference[oaicite:18]{index=18}\n",
        "    r1 = r[K1]  # shape (m,Na,Na)\n",
        "    # Rij = sum_k (2*mu*sigma0^2*r*s + sigma_s2*r^2*s) / (m*mu^2*sigma_s2)\n",
        "    # since s=1 for these samples, we can drop s in multiplication\n",
        "    num = np.sum(2.0 * mu * sigma0_2 * r1 + sigma_s2 * (r1 ** 2), axis=0)\n",
        "    Rij = num / (m * (mu**2) * sigma_s2)\n",
        "\n",
        "    # hopt per APD\n",
        "    hopt = np.vectorize(_hopt_from_Rij)(Rij, mu, sigma_s2, sigma0_2)\n",
        "    hopt[hopt < 0] = 0.0  # physical clamp\n",
        "\n",
        "    # A2 (eq. 43): sum over k in K1 of ln(sigma_s2*h + sigma0_2) + (r - mu*h)^2/(sigma_s2*h + sigma0_2)\n",
        "    v = sigma_s2 * hopt + sigma0_2\n",
        "    metric += m * np.sum(np.log(v))\n",
        "    metric += np.sum(((r1 - mu * hopt) ** 2) / v)\n",
        "\n",
        "    return float(metric)\n",
        "\n",
        "\n",
        "def detect_glrt_reduced_search(\n",
        "    r: np.ndarray,            # (L,Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    Reduced search described after eq. (45): choose candidate sequences where the\n",
        "    m largest received levels correspond to 1s (m=1..L). :contentReference[oaicite:19]{index=19}\n",
        "\n",
        "    We use the total array current per time k:\n",
        "      y[k] = sum_{i,j} r_ij[k]  (matches EGC nature of \"largest levels\"). :contentReference[oaicite:20]{index=20}\n",
        "    \"\"\"\n",
        "    L = r.shape[0]\n",
        "    y = np.sum(r, axis=(1, 2))  # (L,)\n",
        "\n",
        "    order = np.argsort(-y)  # descending\n",
        "    best_metric = np.inf\n",
        "    best_s = None\n",
        "\n",
        "    # m from 1..L (paper’s reduced search space is L candidates) :contentReference[oaicite:21]{index=21}\n",
        "    for m in range(1, L + 1):\n",
        "        s_cand = np.zeros(L, dtype=np.int8)\n",
        "        s_cand[order[:m]] = 1\n",
        "\n",
        "        met = _glrt_metric_for_candidate_s(r, s_cand, mu, sigma_s2, sigma0_2)\n",
        "        if met < best_metric:\n",
        "            best_metric = met\n",
        "            best_s = s_cand\n",
        "\n",
        "    return best_s\n",
        "def detect_egc_glrt_reduced_search(\n",
        "    r: np.ndarray,       # (L,Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    EGC baseline that IMPROVES with L (like Fig.5):\n",
        "    - combine: y[k] = sum_ij r_ij[k]\n",
        "    - treat as scalar channel with ht unknown but constant over the window:\n",
        "        y[k] = mu*s[k]*ht + n[k]\n",
        "      Var(n[k]) = sigma_s2*ht*s[k] + Na^2*sigma0_2   (from eq.36 discussion) :contentReference[oaicite:4]{index=4}\n",
        "    - apply the same reduced-search idea (search m=1..L candidates) :contentReference[oaicite:5]{index=5}\n",
        "    \"\"\"\n",
        "    L, Na, _ = r.shape\n",
        "    y = np.sum(r, axis=(1, 2))        # scalar sequence\n",
        "\n",
        "    # scalar-noise baseline after summing Na^2 APDs\n",
        "    sigma0_sum2 = (Na**2) * sigma0_2\n",
        "\n",
        "    order = np.argsort(-y)  # descending\n",
        "    best_metric = np.inf\n",
        "    best_s = None\n",
        "\n",
        "    for m in range(1, L + 1):\n",
        "        s_cand = np.zeros(L, dtype=np.int8)\n",
        "        s_cand[order[:m]] = 1\n",
        "\n",
        "        # Compute scalar GLRT metric by doing ML over ht for this candidate s.\n",
        "        K1 = (s_cand == 1)\n",
        "        K0 = ~K1\n",
        "        m_eff = int(np.sum(K1))\n",
        "        if m_eff == 0:\n",
        "            continue\n",
        "\n",
        "        # metric part for zeros (Gaussian with variance sigma0_sum2)\n",
        "        metric = 0.0\n",
        "        metric += (L - m_eff) * np.log(sigma0_sum2)\n",
        "        metric += np.sum((y[K0] ** 2) / sigma0_sum2)\n",
        "\n",
        "        # Estimate ht (scalar analog of eq.44) using y[k] for k in K1 :contentReference[oaicite:6]{index=6}\n",
        "        y1 = y[K1]\n",
        "        num = np.sum(2.0 * mu * sigma0_sum2 * y1 + sigma_s2 * (y1 ** 2))\n",
        "        Rij = num / (m_eff * (mu**2) * sigma_s2)\n",
        "\n",
        "        ht_opt = _hopt_from_Rij(Rij, mu, sigma_s2, sigma0_sum2)\n",
        "        ht_opt = max(ht_opt, 0.0)\n",
        "\n",
        "        v = sigma_s2 * ht_opt + sigma0_sum2\n",
        "        metric += m_eff * np.log(v)\n",
        "        metric += np.sum(((y1 - mu * ht_opt) ** 2) / v)\n",
        "\n",
        "        if metric < best_metric:\n",
        "            best_metric = metric\n",
        "            best_s = s_cand\n",
        "\n",
        "    return best_s\n",
        "def detect_egc_estimated_ht_symbolwise(\n",
        "    r: np.ndarray,      # (L,Na,Na)\n",
        "    mu: float,\n",
        "    sigma_s2: float,\n",
        "    sigma0_2: float,\n",
        ") -> np.ndarray:\n",
        "    \"\"\"\n",
        "    EGC benchmark that *does* improve with L:\n",
        "      - Form y[k] = sum_ij r_ij[k] (Eq. 36) :contentReference[oaicite:17]{index=17}\n",
        "      - Estimate ht from the observation window (slow fading over L) :contentReference[oaicite:18]{index=18}\n",
        "      - Use Eq. (40) threshold with estimated ht :contentReference[oaicite:19]{index=19}\n",
        "      - Decide symbol-by-symbol using Eq. (38)\n",
        "\n",
        "    Why this is needed:\n",
        "      Fig. 5 explicitly states EGC performance improves with L. :contentReference[oaicite:20]{index=20}\n",
        "      Pure symbolwise EGC with *known* ht doesn’t depend on L (Eq. 38–40),\n",
        "      so we give EGC an L-dependent ht estimate (very low complexity).\n",
        "    \"\"\"\n",
        "    L, Na, _ = r.shape\n",
        "    y = np.sum(r, axis=(1, 2))  # y[k] in Eq. (36) :contentReference[oaicite:21]{index=21}\n",
        "\n",
        "    # Simple blind estimate from window average:\n",
        "    # For equiprobable OOK (p0=p1=1/2), E[y] ≈ (1/2) * mu * ht.\n",
        "    # => ht_hat ≈ 2 * mean(y) / mu\n",
        "    ht_hat = 2.0 * float(np.mean(y)) / max(mu, 1e-30)\n",
        "    ht_hat = max(ht_hat, 0.0)\n",
        "\n",
        "    thr = egc_threshold(ht_hat, Na, mu, sigma_s2, sigma0_2)\n",
        "    return (y > thr).astype(np.int8)\n"
      ],
      "metadata": {
        "id": "1KoM02ZoKLsl"
      },
      "execution_count": 38,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# run_ber_vs_L.py\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Paper-based parameters (Eq. 18–19 + Table I)\n",
        "# -----------------------------\n",
        "def cubesat_params_from_paper(F: float = 3.0):\n",
        "    \"\"\"\n",
        "    CubeSat paper:\n",
        "      mu = e G eta / (h nu)\n",
        "      Var(n_ij[k]) = sigma_s^2 * h_ij * s[k] + sigma0^2\n",
        "      sigma0^2 = sigma_b^2 + sigma_th^2\n",
        "      sigma_b^2 = 2 e G F mu B P_b\n",
        "      sigma_th^2 = 4 k_B T_r B / R_l\n",
        "\n",
        "    Here we use:\n",
        "      sigma_s^2 = 2 e G F mu B     (coefficient; do NOT multiply by Pt here)\n",
        "    \"\"\"\n",
        "    # constants\n",
        "    e = 1.602176634e-19\n",
        "    h = 6.62607015e-34\n",
        "    c = 299792458.0\n",
        "    kB = 1.380649e-23\n",
        "\n",
        "    # ---- Table I (as used in your paper) ----\n",
        "    G = 100\n",
        "    eta = 0.7\n",
        "    lam = 1550e-9\n",
        "    Rl = 1e3\n",
        "    Tr = 300.0\n",
        "    Tb = 1e-9\n",
        "    Pb = 1e-9\n",
        "\n",
        "    nu = c / lam\n",
        "    mu = (e * G * eta) / (h * nu)\n",
        "\n",
        "    # Common NRZ assumption for electrical bandwidth\n",
        "    B = 1.0 / (2.0 * Tb)\n",
        "\n",
        "    sigma_b2 = 2.0 * e * G * F * mu * B * Pb\n",
        "    sigma_th2 = 4.0 * kB * Tr * B / Rl\n",
        "    sigma0_2 = sigma_b2 + sigma_th2\n",
        "\n",
        "    # shot-noise coefficient (no Pt here; Pt is in s[k])\n",
        "    sigma_s2 = 2.0 * e * G * F * mu * B\n",
        "\n",
        "    return mu, sigma_s2, sigma0_2\n",
        "\n",
        "\n",
        "def Pt_W_from_dB(Pt_dB: float, assume: str = \"dBm\") -> float:\n",
        "    \"\"\"\n",
        "    Paper labels Pt as '5 dB' and '10 dB' (Fig. 5) but doesn't clearly state reference.\n",
        "    Try dBm first; if results are off, switch to 'dBW'.\n",
        "    \"\"\"\n",
        "    if assume.lower() == \"dmb\" or assume.lower() == \"dbm\":\n",
        "        return 1e-3 * 10.0 ** (Pt_dB / 10.0)  # dBm -> W\n",
        "    if assume.lower() == \"dbw\":\n",
        "        return 1.0 * 10.0 ** (Pt_dB / 10.0)   # dBW -> W\n",
        "    raise ValueError(\"assume must be 'dBm' or 'dBW'\")\n",
        "\n",
        "\n",
        "def ber_vs_L_demo(Pt_dB=5.0, n_trials=2000, seed=0, pt_ref=\"dBm\"):\n",
        "    rng = np.random.default_rng(seed)\n",
        "\n",
        "    Na = 10\n",
        "    wa = 250e-6\n",
        "    wf = 5e-6\n",
        "    fc = 0.03\n",
        "    sigma_I = 150e-6\n",
        "    sigma_theta = 5e-3\n",
        "\n",
        "    mu, sigma_s2, sigma0_2 = cubesat_params_from_paper()\n",
        "    Pt_W = Pt_W_from_dB(Pt_dB, assume=pt_ref)\n",
        "\n",
        "    L_list = list(range(2, 22, 2))\n",
        "\n",
        "    ber_ideal = []\n",
        "    ber_egc = []\n",
        "    ber_prop = []\n",
        "\n",
        "    for L in L_list:\n",
        "        err_id = err_eg = err_pr = 0\n",
        "        total_bits = 0\n",
        "\n",
        "        for _ in range(n_trials):\n",
        "            theta_x = rng.normal(0.0, sigma_theta)\n",
        "            theta_y = rng.normal(0.0, sigma_theta)\n",
        "\n",
        "            h1 = 1.0\n",
        "            hij = channel_hij(Na, wa, wf, fc, sigma_I, theta_x, theta_y, h1)\n",
        "\n",
        "            b, r = simulate_window(L, hij, mu, sigma_s2, sigma0_2, rng, Pt_W=Pt_W)\n",
        "\n",
        "            b_id = detect_ideal_symbolwise(r, hij, mu, sigma_s2, sigma0_2)\n",
        "            b_eg = detect_egc_estimated_ht_symbolwise(r, mu, sigma_s2, sigma0_2)\n",
        "            b_pr = detect_glrt_reduced_search(r, mu, sigma_s2, sigma0_2)\n",
        "\n",
        "            err_id += np.sum(b_id != b)\n",
        "            err_eg += np.sum(b_eg != b)\n",
        "            err_pr += np.sum(b_pr != b)\n",
        "            total_bits += L\n",
        "\n",
        "        ber_ideal.append(err_id / total_bits)\n",
        "        ber_egc.append(err_eg / total_bits)\n",
        "        ber_prop.append(err_pr / total_bits)\n",
        "\n",
        "    return L_list, ber_ideal, ber_egc, ber_prop\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "\n",
        "    # Run simulations\n",
        "    L, ber_i_5, ber_e_5, ber_p_5 = ber_vs_L_demo(Pt_dB=5, seed=0)\n",
        "    L, ber_i_10, ber_e_10, ber_p_10 = ber_vs_L_demo(Pt_dB=10, seed=1)\n",
        "\n",
        "    # Create ONE figure\n",
        "    plt.figure(figsize=(8, 6))\n",
        "\n",
        "    # ---- Pt = 5 dB ----\n",
        "    plt.semilogy(L, ber_i_5, \"o-\", label=\"Ideal, Pt=5 dB\")\n",
        "    plt.semilogy(L, ber_e_5, \"s-\", label=\"EGC, Pt=5 dB\")\n",
        "    plt.semilogy(L, ber_p_5, \"^-\", label=\"Proposed, Pt=5 dB\")\n",
        "\n",
        "    # ---- Pt = 10 dB ----\n",
        "    plt.semilogy(L, ber_i_10, \"o--\", label=\"Ideal, Pt=10 dB\")\n",
        "    plt.semilogy(L, ber_e_10, \"s--\", label=\"EGC, Pt=10 dB\")\n",
        "    plt.semilogy(L, ber_p_10, \"^--\", label=\"Proposed, Pt=10 dB\")\n",
        "\n",
        "    # Formatting\n",
        "    plt.xlabel(\"Observation window length L\")\n",
        "    plt.ylabel(\"BER\")\n",
        "    plt.ylim(1e-6, 1)          # expand y-axis\n",
        "    plt.grid(True, which=\"both\")\n",
        "    plt.legend()\n",
        "    plt.title(\"BER vs L for Pt = 5 dB and 10 dB\")\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n"
      ],
