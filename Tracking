      "source": [
        "import importlib.util\n",
        "import numpy as np\n",
        "from math import sqrt, erf\n",
        "\n",
        "# =============================================================================\n",
        "# 0) Load Mansour functions (by file path)\n",
        "# =============================================================================\n",
        "import importlib.util\n",
        "\n",
        "MANSOUR_PATH = \"/content/Mansour functions.py\"\n",
        "\n",
        "spec = importlib.util.spec_from_file_location(\"mansour\", MANSOUR_PATH)\n",
        "mansour = importlib.util.module_from_spec(spec)\n",
        "spec.loader.exec_module(mansour)\n",
        "print(\"KHAAARRRRR\", dir(mansour))\n",
        "\n",
        "# =============================================================================\n",
        "# 1) User configuration switches\n",
        "# =============================================================================\n",
        "\n",
        "# --- Attenuation model selection ---\n",
        "USE_KIM_FOG_MODEL = False   # True: use mansour.atmospheric_attenuation_kim(...)\n",
        "USE_BEER_LAMBERT  = True    # True: use exp(-zeta*L) instead\n",
        "# If both are True, Kim fog takes precedence.\n",
        "def zeta_from_dB_per_km(dB_per_km: float) -> float:\n",
        "    \"\"\"Convert attenuation from dB/km to 1/m.\"\"\"\n",
        "    return (dB_per_km / 10.0) * np.log(10) / 1000.0\n",
        "\n",
        "# Beer–Lambert parameter (1/m)\n",
        "ZETA_PER_M = zeta_from_dB_per_km(1.0)\n",
        "# ≈ 2.3e-4\n",
        "\n",
        "\n",
        "# Kim fog parameters (only used if USE_KIM_FOG_MODEL=True)\n",
        "FOG_VIS_KM = 10.0           # visibility in km\n",
        "FOG_T_TH   = 0.02           # threshold parameter used in the Kim model\n",
        "LAMBDA0_M  = 550e-9         # reference wavelength in meters (often 550 nm)\n",
        "\n",
        "# --- Turbulence model selection (Mansour) ---\n",
        "TURB_MODEL = \"GG\"           # \"GG\" or \"LN\"\n",
        "\n",
        "# --- Pointing model selection (Mansour) ---\n",
        "# The Mansour function returns a time-series hp[k] using their PE parameters.\n",
        "\n",
        "# --- Lens->APD array model (you add h2 here) ---\n",
        "Na = 4          # APD array: Na x Na (example: 4x4)\n",
        "wa = 50e-6      # pixel width (meters)\n",
        "fc = 0.02       # focal length (meters)\n",
        "wf = 200e-6     # Gaussian spot radius on focal plane (meters), 1/e^2 intensity radius\n",
        "\n",
        "# AoA time-series (radians) feeding h2 (you can replace with your tracking model later)\n",
        "# For now we do simple zero-mean Gaussian AoA jitter:\n",
        "AOA_STD_RAD = 50e-6         # example small jitter in radians\n",
        "AOA_MEAN_X  = 0.0\n",
        "AOA_MEAN_Y  = 0.0\n",
        "\n",
        "# --- Received signal demo (optional) ---\n",
        "DO_RX_DEMO = True\n",
        "B_DETECT = 2     # b=1 for HD, b=2 for DD\n",
        "NOISE_STD = 0.01\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# 2) Helper: h2 (lens -> APD array) from 2D Gaussian footprint\n",
        "# =============================================================================\n",
        "def apd_pixel_gains_gaussian(xb: float, yb: float, wf: float, wa: float, Na: int) -> np.ndarray:\n",
        "    \"\"\"\n",
        "    Fractional power per pixel for a circular 2D Gaussian beam centered at (xb, yb) on the focal plane.\n",
        "\n",
        "    Intensity model:\n",
        "      I(x,y) ∝ exp(-2 * ((x-xb)^2 + (y-yb)^2) / wf^2 )\n",
        "\n",
        "    For separable integration we use sigma such that:\n",
        "      exp(-2 r^2 / wf^2) == exp(-r^2 / (2 sigma^2))  -> sigma = wf/2\n",
        "\n",
        "    Returns:\n",
        "      H: (Na, Na) nonnegative, sum(H)=1\n",
        "    \"\"\"\n",
        "    sigma = wf / 2.0\n",
        "    s2 = sqrt(2.0) * sigma\n",
        "\n",
        "    # Pixel edge coordinates. Array is centered at 0.\n",
        "    x_edges = (np.arange(Na + 1) - Na / 2.0) * wa\n",
        "    y_edges = (np.arange(Na + 1) - Na / 2.0) * wa\n",
        "\n",
        "    # Gaussian integral over [a,b] proportional to erf((b-mu)/(sqrt(2)sigma)) - erf((a-mu)/(sqrt(2)sigma))\n",
        "    Fx = np.array([erf((xe - xb) / s2) for xe in x_edges])\n",
        "    Fy = np.array([erf((ye - yb) / s2) for ye in y_edges])\n",
        "\n",
        "    gx = Fx[1:] - Fx[:-1]  # length Na\n",
        "    gy = Fy[1:] - Fy[:-1]  # length Na\n",
        "\n",
        "    H = np.outer(gx, gy)   # Na x Na\n",
        "\n",
        "    s = float(H.sum())\n",
        "    if s > 0:\n",
        "        H /= s\n",
        "    return H\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# 3) Build Mansour parameter dictionary and initialise\n",
        "# =============================================================================\n",
        "def build_params():\n",
        "    p = dict(\n",
        "        # Time / sequence sizes\n",
        "        BR=1e6,            # bit rate\n",
        "        NoS=10,            # samples per bit\n",
        "        NoB=1000,          # number of bits\n",
        "\n",
        "        # Update rates for turbulence / pointing series in Mansour framework\n",
        "        F_t=200,           # turbulence \"update\" freq (Hz)\n",
        "        F_p=200,           # pointing \"update\" freq (Hz)\n",
        "\n",
        "        # Tx aperture config\n",
        "        TX_AP_Type=\"ANG\",  # \"ANG\" or \"DIA\"\n",
        "        theta_d_v=2.0,     # degrees (if TX_AP_Type=\"ANG\")\n",
        "        theta_d_h=2.0,     # degrees\n",
        "        w_tx_v=0.02,       # meters (if TX_AP_Type=\"DIA\")\n",
        "        w_tx_h=0.02,       # meters\n",
        "\n",
        "        # Channel\n",
        "        wavelength=1550e-9, # Renamed from 'lambda' to 'wavelength'\n",
        "        Link_Len=1000.0,   # meters\n",
        "        Cn2=1e-14,         # turbulence strength\n",
        "        Rx_Ap_dia=0.05,    # WARNING: in the Mansour file it's used like a radius in area=pi*dia^2\n",
        "\n",
        "        # Pointing stats used by Mansour PE function\n",
        "        sig_j_PE=0.001,\n",
        "        mu_h_PE=0.0,\n",
        "        mu_v_PE=0.0,\n",
        "\n",
        "        # Loop config fields required by initialise_parameters (even if you won't sweep)\n",
        "        Sim_Par=\"POW\",\n",
        "        Avg_Opt_Pow_step=1, Avg_Opt_Pow_start=1.0, Avg_Opt_Pow_end=1.0, Avg_Opt_Pow_const=1.0,\n",
        "        ext_ratio_step=1, ext_ratio_start=10.0, ext_ratio_end=10.0, ext_ratio_const=10.0,\n",
        "\n",
        "        # Receiver/noise placeholders required by initialise_parameters\n",
        "        BW_BR_r=1.0,\n",
        "        PD_NEP=0.0, PD_Gain=1.0, q_ch=1.6e-19, PD_Resp=1.0, P_background=0.0, PD_RL=50.0,\n",
        "    )\n",
        "    # Add 'lambda' key which is expected by mansour.initialise_parameters\n",
        "    p['lambda'] = p['wavelength']\n",
        "    return mansour.initialise_parameters(p)\n",
        "\n",
        "\n",
        "# =============================================================================\n",
        "# 4) Main: generate h_l, h_a, h_p, build h1 and h2, then full h_ij[k]\n",
        "# =============================================================================\n",
        "def main():\n",
        "    p = build_params()\n",
        "    NoP = p[\"NoP\"]\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (A) h_l : attenuation\n",
        "    # ---------------------------------------------------------\n",
        "    if USE_KIM_FOG_MODEL:\n",
        "        # Mansour Kim fog attenuation (returns multiplicative attenuation factor)\n",
        "        # Link length is in meters; vis is in km; wavelength in meters\n",
        "        h_l = mansour.atmospheric_attenuation_kim(\n",
        "            vis_km=FOG_VIS_KM,\n",
        "            t_th=FOG_T_TH,\n",
        "            wavelength_m=p[\"wavelength\"], # Updated from p[\"lambda\"]\n",
        "            lambda0_m=LAMBDA0_M,\n",
        "            link_len_m=p[\"Link_Len\"],\n",
        "        )\n",
        "        h_l = float(h_l)\n",
        "        attenuation_label = \"Kim fog model\"\n",
        "    elif USE_BEER_LAMBERT:\n",
        "        h_l = float(np.exp(-ZETA_PER_M * p[\"Link_Len\"]))\n",
        "        attenuation_label = \"Beer–Lambert\"\n",
        "    else:\n",
        "        h_l = 1.0\n",
        "        attenuation_label = \"none (h_l=1)\"\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (B) h_a[k] : turbulence fading time-series\n",
        "    # ---------------------------------------------------------\n",
        "    h_a = mansour.turbulence_effect(\n",
        "        Turb_Mod=TURB_MODEL,\n",
        "        NoT=p[\"NoT\"],\n",
        "        NoP=NoP,\n",
        "        Resamp_Turb=\"RES\",\n",
        "        mu_x_turb=p[\"mu_x_turb\"],\n",
        "        sig_x_turb=p[\"sig_x_turb\"],\n",
        "        PHI=p[\"PHI\"],\n",
        "        SAI=p[\"SAI\"],\n",
        "    )\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (C) h_p[k] : pointing-loss time-series\n",
        "    # ---------------------------------------------------------\n",
        "    h_p = mansour.pointing_errors_effect(\n",
        "        NoPE=p[\"NoPE\"],\n",
        "        NoP=NoP,\n",
        "        sig_j_PE=p[\"sig_j_PE\"],\n",
        "        mu_h_PE=p[\"mu_h_PE\"],\n",
        "        mu_v_PE=p[\"mu_v_PE\"],\n",
        "        W2_eq_PE_h=p[\"W2_eq_PE_h\"],\n",
        "        W2_eq_PE_v=p[\"W2_eq_PE_v\"],\n",
        "        A0_PE_h=1.0,\n",
        "        A0_PE_v=1.0,\n",
        "        Resamp_PE=\"RES\",\n",
        "    )\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (D) h1[k] = h_l * h_a[k] * h_p[k]\n",
        "    # ---------------------------------------------------------\n",
        "    h1 = h_l * h_a * h_p\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (E) Build AoA series (theta_x[k], theta_y[k]) for h2\n",
        "    #     Replace this with your tracker output later.\n",
        "    # ---------------------------------------------------------\n",
        "    rng = np.random.default_rng(0)\n",
        "    theta_x = AOA_MEAN_X + AOA_STD_RAD * rng.standard_normal(NoP)\n",
        "    theta_y = AOA_MEAN_Y + AOA_STD_RAD * rng.standard_normal(NoP)\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (F) h2[k, i, j] from AoA -> spot center -> pixel integrals\n",
        "    #     h[k,i,j] = h1[k] * h2[k,i,j]\n",
        "    # ---------------------------------------------------------\n",
        "    h2 = np.zeros((NoP, Na, Na), dtype=float)\n",
        "    h = np.zeros((NoP, Na, Na), dtype=float)\n",
        "\n",
        "    for k in range(NoP):\n",
        "        xb = fc * theta_x[k]\n",
        "        yb = fc * theta_y[k]\n",
        "        h2[k] = apd_pixel_gains_gaussian(xb, yb, wf=wf, wa=wa, Na=Na)\n",
        "        h[k] = h1[k] * h2[k]\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # Print quick stats\n",
        "    # ---------------------------------------------------------\n",
        "    print(\"==== Channel summary ====\")\n",
        "    print(f\"Attenuation model: {attenuation_label}, h_l = {h_l:.6g}\")\n",
        "    print(f\"Turbulence model: {TURB_MODEL}\")\n",
        "    print(f\"NoP (time samples): {NoP}\")\n",
        "    print(f\"h1 mean/var: {float(np.mean(h1)):.6g} / {float(np.var(h1)):.6g}\")\n",
        "    print(f\"h2[0] sum: {float(h2[0].sum()):.6g}  (should be 1)\")\n",
        "    print(f\"h best-pixel mean/var (over k): \", end=\"\")\n",
        "\n",
        "    # best pixel each sample\n",
        "    j_star = np.argmax(h.reshape(NoP, -1), axis=1)\n",
        "    h_best = h.reshape(NoP, -1)[np.arange(NoP), j_star]\n",
        "    print(f\"{float(np.mean(h_best)):.6g} / {float(np.var(h_best)):.6g}\")\n",
        "\n",
        "    # ---------------------------------------------------------\n",
        "    # (G) Optional received-signal demo: y = h^(b/2) x + n\n",
        "    # ---------------------------------------------------------\n",
        "    if DO_RX_DEMO:\n",
        "        b = B_DETECT\n",
        "        x = rng.choice([-1.0, 1.0], size=NoP)        # placeholder symbols\n",
        "        n = NOISE_STD * rng.standard_normal(NoP)\n",
        "\n",
        "        y = (h_best ** (b / 2.0)) * x + n\n",
        "        print(\"\\n==== Rx demo ====\")\n",
        "        print(f\"Detection type: {'HD (b=1)' if b==1 else 'DD (b=2)'}\")\n",
        "        print(f\"y mean/var: {float(np.mean(y)):.6g} / {float(np.var(y)):.6g}\")\n",
        "\n",
        "    # If you want to export h1/h2/h for plotting later, you can save:\n",
        "    # np.savez(\"channel_out.npz\", h1=h1, h2=h2, h=h, theta_x=theta_x, theta_y=theta_y)\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from math import sqrt\n",
        "from scipy.special import erf # Changed from math.erf to scipy.special.erf\n",
        "import numpy as np # Added import for numpy\n",
        "\n",
        "def apd_pixel_gains_gaussian(xb, yb, wf, wa, Na):\n",
        "    sigma = wf / 2.0\n",
        "    s2 = sqrt(2.0) * sigma\n",
        "\n",
        "    x_edges = (np.arange(Na + 1) - Na / 2.0) * wa\n",
        "    y_edges = (np.arange(Na + 1) - Na / 2.0) * wa\n",
        "\n",
        "    Fx = erf((x_edges - xb) / s2)\n",
        "    Fy = erf((y_edges - yb) / s2)\n",
        "\n",
        "    gx = Fx[1:] - Fx[:-1]\n",
        "    gy = Fy[1:] - Fy[:-1]\n",
        "\n",
        "    H = np.outer(gx, gy)\n",
        "    return H / H.sum()\n",
        "\n",
        "\n",
        "def run_channel(\n",
        "    NoP=20000,\n",
        "    Pt_dB=0,\n",
        "    Na=4,\n",
        "    Z=1000,\n",
        "    fog_att_db_km=0.0,\n",
        "    AOA_std=50e-6\n",
        "):\n",
        "    # -------------------------\n",
        "    # Mansour parameters\n",
        "    # -------------------------\n",
        "    p = dict(\n",
        "        BR=1e6, NoS=10, NoB=1000,\n",
        "        F_t=200, F_p=200,\n",
        "        TX_AP_Type=\"ANG\",\n",
        "        theta_d_v=2.0, theta_d_h=2.0,\n",
        "        wavelength=1550e-9,\n",
        "        Link_Len=Z,\n",
        "        Cn2=1e-14,\n",
        "        Rx_Ap_dia=0.05,\n",
        "        sig_j_PE=0.001,\n",
        "        mu_h_PE=0.0,\n",
        "        mu_v_PE=0.0,\n",
        "        Sim_Par=\"POW\",\n",
        "        Avg_Opt_Pow_step=1,\n",
        "        Avg_Opt_Pow_start=1.0,\n",
        "        Avg_Opt_Pow_end=1.0,\n",
        "        Avg_Opt_Pow_const=1.0,\n",
        "        ext_ratio_step=1,\n",
        "        ext_ratio_start=10.0,\n",
        "        ext_ratio_end=10.0,\n",
        "        ext_ratio_const=10.0,\n",
        "        BW_BR_r=1.0,\n",
        "        PD_NEP=0.0, PD_Gain=1.0,\n",
        "        q_ch=1.6e-19,\n",
        "        PD_Resp=1.0,\n",
        "        P_background=0.0,\n",
        "        PD_RL=50.0\n",
        "    )\n",
        "\n",
        "    # Add 'lambda' key which is expected by mansour.initialise_parameters\n",
        "    p['lambda'] = p['wavelength']\n",
        "    p = mansour.initialise_parameters(p)\n",
        "    NoP = p[\"NoP\"]\n",
        "\n",
        "    # -------------------------\n",
        "    # Channel components\n",
        "    # -------------------------\n",
        "    h_a = mansour.turbulence_effect(\n",
        "        \"GG\", p[\"NoT\"], NoP, \"RES\",\n",
        "        p[\"mu_x_turb\"], p[\"sig_x_turb\"],\n",
        "        p[\"PHI\"], p[\"SAI\"]\n",
        "    )\n",
        "\n",
        "    h_p = mansour.pointing_errors_effect(\n",
        "        p[\"NoPE\"], NoP,\n",
        "        p[\"sig_j_PE\"],\n",
        "        p[\"mu_h_PE\"], p[\"mu_v_PE\"],\n",
        "        p[\"W2_eq_PE_h\"], p[\"W2_eq_PE_v\"],\n",
        "        1.0, 1.0, \"RES\"\n",
        "    )\n",
        "\n",
        "    # Beer–Lambert fog\n",
        "    Z_km = Z / 1000\n",
        "    h_fog = 10 ** (-(fog_att_db_km * Z_km) / 20)\n",
        "\n",
        "    h1 = h_fog * h_a * h_p\n",
        "\n",
        "    # -------------------------\n",
        "    # AoA → APD array\n",
        "    # -------------------------\n",
        "    fc = 0.02\n",
        "    wa = 50e-6\n",
        "    wf = 200e-6\n",
        "\n",
        "    theta_x = AOA_std * np.random.randn(NoP)\n",
        "    theta_y = AOA_std * np.random.randn(NoP)\n",
        "\n",
        "    h = np.zeros((NoP, Na, Na))\n",
        "    for k in range(NoP):\n",
        "        xb = fc * theta_x[k]\n",
        "        yb = fc * theta_y[k]\n",
        "        h2 = apd_pixel_gains_gaussian(xb, yb, wf, wa, Na)\n",
        "        h[k] = h1[k] * h2\n",
        "\n",
        "    # best pixel (tracking)\n",
        "    h_best = np.max(h.reshape(NoP, -1), axis=1)\n",
        "\n",
        "    return {\n",
        "        \"h\": h,\n",
        "        \"h_best\": h_best,\n",
        "        \"h1\": h1,\n",
        "        \"theta_x\": theta_x,\n",
        "        \"theta_y\": theta_y\n",
        "    }"
      ],
      "metadata": {
        "id": "Xbhl22iNQLCg"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "Pt_dB = np.arange(-10, 16, 5)\n",
        "BER = []\n",
        "\n",
        "for pt in Pt_dB:\n",
        "    out = run_channel(Pt_dB=pt)\n",
        "    h = out[\"h_best\"]\n",
        "\n",
        "    gamma_bar = 10 ** (pt / 10)\n",
        "    gamma = gamma_bar * h**2  # DD\n",
        "\n",
        "    from scipy.special import erfc\n",
        "    ber = np.mean(0.5 * erfc(np.sqrt(gamma / 8)))\n",
        "    BER.append(ber)\n",
        "\n",
        "plt.semilogy(Pt_dB, BER, 'o-')\n",
        "plt.xlabel(\"Transmit power Pt (dB)\")\n",
        "plt.ylabel(\"BER\")\n",
